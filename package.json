{
    "name": "yd-vue-ssr",
    "version": "1.0.0",
    "main": "gulpfile.js",
    "bin":{
        "ydcli": "bin/index.js"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=production pm2 start app.js -i max",
        "test": "npm run unit && npm run e2e && npm run service",
        "build": "npm run client && npm run client:server && npm run gulp:prod",
        "devserver": "cross-env NODE_ENV=development supervisor ./build/app.js",
        "unit": "cross-env NODE_ENV=development karma start",
        "e2e": "cross-env NODE_ENV=development node nightwatch.runner.js",
        "permance": "webpack --profile --json --config ./config/webpack.prod.js > ./test/permance/stats.json",
        "service": "./node_modules/.bin/mocha  ./test/service/router.spec.js --reporter  mochawesome --reporter-options reportDir=./reports/mochawesome-report",
        "client:dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.dev.js --progress --colors",
        "client:prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js --progress --hide-modules",
        "client:server": "cross-env NODE_ENV=development webpack --config ./config/webpack.server.js --progress --hide-modules",
        "gulp:prod": "cross-env NODE_ENV=production gulp buildprod"
    },
    "author": "yuanzhijia@yidengxuetang.com",
    "license": "ISC",
    "description": "",
